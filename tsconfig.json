{
  "compilerOptions": {
    "module": "amd",
    "lib": ["es6", "dom"],
    "noImplicitAny": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "jsx": "react",
    "jsxFactory": "tsx",
    "target": "ES5",
    "experimentalDecorators": true,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "outDir": "ael"
  },
  "include": [
    "./typescript/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
